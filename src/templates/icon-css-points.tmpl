// Import Font

@font-face {
  font-family: "<%= fontName %>";
  src:
    data-uri("../build/fonts/<%= fontName %>.woff") format("woff"),
    url("../build/fonts/<%= fontName %>.ttf") format("truetype");
  font-weight: normal;
  font-style: normal;
}


// Icon Weight

.icon-lg,
.icon-sm {
  font-family: "<%= fontName %>";
}


// Icon Points

<% _.each(glyphs, function(glyph) { %>.<%= className %>-<%= glyph.name %>:before {
  content: "\<%= glyph.codepoint.toString(16) %>";
}

<% }); %>
